{
  "name": "nexus-dawn",
  "private": true,
  "version": "0.1.0",
  "description": "An emotionally intelligent, modular AI operating system",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md,json}\"",
    "prepare": "husky install",
    "type-check": "tsc --noEmit",
    "validate": "npm-run-all --parallel type-check lint test build"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@google/generative-ai": "^0.1.0",
    "@livekit/components-styles": "^1.0.0",
    "@mui/material": "^7.1.1",
    "@mui/system": "^7.1.1",
    "@react-three/drei": "^9.0.0",
    "@react-three/fiber": "^8.0.0",
    "@reduxjs/toolkit": "^2.0.0",
    "algosdk": "^2.5.0",
    "axios": "^1.6.0",
    "crypto-js": "^4.2.0",
    "dompurify": "^3.2.6",
    "face-api.js": "^0.22.2",
    "framer-motion": "^10.0.0",
    "livekit-client": "^2.13.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.0.0",
    "react-router-dom": "^6.0.0",
    "three": "^0.150.0",
    "uuid": "^11.1.0",
    "zod": "^3.25.51",
    "zustand": "^4.0.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@livekit/components-core": "^0.12.7",
    "@livekit/components-react": "^2.9.9",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/crypto-js": "^4.2.2",
    "@types/dompurify": "^3.0.5",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "@types/three": "^0.150.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.0.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.0.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "husky": "^8.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.0.0",
    "npm-run-all": "^4.0.0",
    "postcss": "^8.0.0",
    "prettier": "^3.0.0",
    "tailwindcss": "^3.0.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,md,json}": [
      "prettier --write"
    ]
  }
}
